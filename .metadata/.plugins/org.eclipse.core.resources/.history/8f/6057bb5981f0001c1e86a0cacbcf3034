package com.example.demo.pojo;

import javax.persistence.Entity;
import javax.persistence.Id;

import lombok.Data;

@Data
@Entity
public class UserPojo {
	
	@Id
	private int id;
	// User Details
	private String name;
	private String email;
	private String Address;
	private String Phone_no;
	
	//Security
	private String Password;
	
	
	
}
